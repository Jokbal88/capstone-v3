{
  "name": "theme",
  "version": "1.0.0",
  "description": "Tailwind CSS theme for medical system",
  "scripts": {
    "build": "tailwindcss -i ./static_src/src/input.css -o ./static/css/output.css --minify",
    "dev": "tailwindcss -i ./static_src/src/input.css -o ./static/css/output.css --watch"
  },
  "dependencies": {
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1"
  }
}
